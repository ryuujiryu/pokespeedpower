mapscripts Route51Sky_MapScripts {
MAP_SCRIPT_ON_TRANSITION {
    call(SetMCInvisible)
    end
    }
MAP_SCRIPT_ON_FRAME_TABLE [
    VAR_RIVALS_STATE, 1: Route51Sky_Rival
    ]
}

script Route51Sky_Rival {
    faceplayer
    release
    end
}

script SetMCInvisible {
    applymovement (LOCALID_PLAYER, Common_Movement_Invisible)
    end
}